{
  "expo": {
    "name": "Vault Password Manager",
    "slug": "vault-mobile",
    "version": "1.0.0",
    "icon": "./assets/vault.png",
    "orientation": "portrait",
    "scheme": "vault",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/vault.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vault.mobile",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.vault.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/vault.png",
        "backgroundColor": "#ffffff"
      },
      "usesCleartextTraffic": true,
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2bf41476-0b47-49fb-af23-8cd3771264d9"
      }
    }
  }
}